    MainSection:NewDropdown("Quick Reset", "Makes you reset", { "RESET"}, function(currentOption)
    print(currentOption)

    game.ReplicatedStorage.Remotes.Reset:FireServer()
    game.VisibilityStateChanged:FireServer("true")
   
end)
