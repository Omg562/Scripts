local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Hades God Hub | The Mystic Diaries", "DarkTheme")

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection(" ")
Section:NewButton("1", "", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/XTheMasterX/Scripts/Main/TheMysticDiaries'),true))()
end)
Section:NewButton("2", "", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/RZmmQXEA', true))()
end)
Section:NewButton("3", "", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Omg562/Scripts/main/TXXMD', true))()
end)
